<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>	
	<bean id = "country" class = "com.cogent.Country">
		<constructor-arg index = "0" value = "USA"></constructor-arg>
		<constructor-arg index = "1" value = "2000"></constructor-arg>
	</bean>
	
	<bean id = "country2" class = "com.cogent.Country">
		<constructor-arg index = "0" value = "China"></constructor-arg>
		<constructor-arg index = "1" value = "40000"></constructor-arg>
	</bean>
	
	<bean id = "country3" class = "com.cogent.Country">
		<constructor-arg index = "0" value = "IN"></constructor-arg>
		<constructor-arg index = "1" value = "60000"></constructor-arg>
	</bean>
	
</beans>